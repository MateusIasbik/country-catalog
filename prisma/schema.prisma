generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model CountryRating {
  id        Int      @id @default(autoincrement())
  country   String
  liked     Boolean
  createdAt DateTime @default(now())
}
